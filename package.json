{
  "name": "swiftly_nodejs_scaffolding",
  "version": "1.0.1",
  "description": "Swiftly Nodejs Scaffolding",
  "repository": {
    "type": "git",
    "url": "https://github.com/iamwilliamdiaz/swiftly_nodejs_scaffolding"
  },
  "engines": {
    "node": ">=12.13.1",
    "npm": ">=6.12.0"
  },
  "main": "index.js",
  "author": "William Diaz",
  "license": "UNLICENSED",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts && npm run tslint",
    "serve": "node dist/index.js",
    "watch-node": "nodemon dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\"  \"npm run watch-ts\" \"npm run watch-node\"",
    "test": "jest --setupFiles dotenv/config --forceExit --coverage --verbose --detectOpenHandles --no-watchman",
    "coverage": "nyc --cache --check-coverage --reporter=lcov npm run test",
    "coverage:report": "nyc report --reporter=lcov --reporter=html",
    "cover:full": "nyc --silent --no-clean npm run test",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "debug": "npm run build && npm run watch-debug",
    "serve-debug": "nodemon dist/index.js",
    "watch-debug": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-ts\" \"npm run serve-debug\"",
    "build-docs": "typedoc --hideGenerator -out docs && typedoc --theme markdown",
    "jenkins-lint": "tslint -t checkstyle --project reports/tslint.xml"
  },
